page{
  font-family: "微软雅黑", Arial, sans-serif;
}
.page-body{
  padding-top: 60rpx;
}
.page-section{
  width: 100%;
  margin-bottom: 60rpx;
}
.page-section-gap{
  box-sizing: border-box;
  padding: 0 30rpx;
}
.page-section-searchHistory{
  display: flex;
  flex-direction: row;
  flex: 0 0 100%;
}
.title{
  justify-content: space-between;
  font-weight: bold;
  /* width: 650rpx; */
}
.deleteImageBox{
  width: 50rpx;
  height: 50rpx;
  /* align-items: flex-end; */
}
.deleteImage{
  width: 60%;
  height: 60%;
  padding: 20%;
}
.page-section-searchBox{
  width: fit-content;
  border-radius: 20px;
  background-color: rgba(226, 221, 221, 0.575);
  display: inline-block;
  white-space: nowrap;
  margin-right: 30rpx; 
}
.page-section-item{
  padding: 20rpx;
  display: flex;
}
.brandImageBox{
  width: 110rpx;
  height: 110rpx;
}
.brandImage{
  width: 85%;
  max-height: 85%;
  margin: 7.5%;
}
.brandNameBox{
  width: 550rpx;
  height: 110rpx;
  display: flex;
  line-height: 100rpx;
  border-bottom: 1rpx solid lightgrey;
  margin-left: 5%;
}
.swiper-item{
  display: box;
  height: 150rpx;
  width: 100%;
  height: 100%;
}
