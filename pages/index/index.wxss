.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx;
  box-sizing: border-box;
}
.list-container{
  padding: 0
}
.list-item{
  height: 100rpx;
  display: flex;
  flex-direction: row;
  padding: 20rpx
}
.left{
  width:100rpx;
  height: 100rpx;
}
.right{
  width: 590rpx;
  height: 100rpx;
  margin-left: 20rpx;
  display: flex;
  flex-direction: column
}
