.container{
  margin:3%;
}
.searchHistoryContainer{
  width: 95%;
  max-height: 30%;
}
.searchHistoryTitle{
  max-height: 40%;
  width: 100%;
  text-align: left;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.searchHistoryTitleText{
  font-weight: bold;
  max-width: 80%;
}
.deleteImage{
  width: 30rpx;
  height: 30rpx;
  align-items: flex-end;
}
.searchHistoryList{
  width: 100%;
  font-family: "微软雅黑", Arial, sans-serif;
  max-height: 60%;
  height: 50%;
  display: flex;
  flex-direction: row;
}
.searchHistoryItemBox{
  width: fit-content;
  height: 78%;
  border-radius: 20px;
  background-color: rgba(231, 228, 228, 0.575);
  display: inline-block;
  white-space: nowrap;
  margin-right: 30rpx;
  margin-top: 15rpx;
  padding-top: 10rpx; 
}
.searchHistoryItem{
  padding: 20rpx;
  width: 80%;
  height: 100%;
  color: black;
  position: relative;
}
.brandContainer{
  margin-top: 60rpx;
}
.brandList{
  height: 100%;
  display: flex;
  align-items: center; 
  justify-content: space-between;
  padding: 20rpx;
  box-sizing: border-box;
}
.brandImageContainer{
  width: 110rpx;
  height: 110rpx;
}
.brandImage{
  width: 100%;
}
.brandName{
  width: 550rpx;
  height: 110rpx;
  display: flex;
  line-height: 100rpx;
  font-family: "微软雅黑", Arial, sans-serif;
  border-bottom: 1rpx solid lightgrey;
  margin-left: 5%;
}





